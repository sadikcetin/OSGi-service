package org.sifirbir.osgi.language.english.impl;

import org.sifirbir.osgi.language.api.ILanguage;

public class EnglishLangImpl implements ILanguage {

	@Override
	public String speak() {
		System.out.println(Configuration.LANG.MESSAGE_SPEAK);
	}

	@Override
	public void state() {
		System.out.println(Configuration.LANG.MESSAGE_STATE);

	}

	@Override
	public String code() {
		return "en";
	}

}
